{# src/SC/ActiviteBundle/Resources/views/InscriptionActivite/viewinscription.html.twig #}
{% extends "SCActiviteBundle:Activite:layout.html.twig" %}
{% block title %}{{ parent() }} - Index{% endblock %}
{% block activité_body %}

    
    {% for inscription in listeDeMesInscriptions %}
        
        <p>      {{ inscription.prenomEnfant }}  {{ inscription.nomEnfant }}  est inscrit à l'activité {{ inscription.activite.nomActivite }}  </p>
        
        <br>
    {% endfor %}  
    {% if prix != 0 %}
    <p> le prix total à payer:(prix activités + licences) est: {{prix}} </p>
    {%endif%}
{% endblock %} 
